@model FocusRandonGenerator.WebApp.Models.GeneratedRandonNumber

@{
    ViewData["Title"] = "CustomRandomNumber";
}
<div class="container">
    <h1>CustomRandomNumber</h1>

    <h4>GeneratedRandonNumber</h4>
    <hr />

     <partial name="_Views_RandomNumber" model="Model" />
   


    <div class="row">
        <div class="col-md-4">
            <form asp-action="CustomRandomNumber">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                <div class="form-group">
                    <label asp-for="NoofRandomnumbers" class="control-label"></label>
                    <input asp-for="NoofRandomnumbers" class="form-control" />
                    <span asp-validation-for="NoofRandomnumbers" class="text-danger"></span>
                </div>

                <div class="form-group">
                    <input type="submit" value="Submit" class="btn btn-primary" />
                </div>
            </form>
        </div>
    </div>
 </div>



    <div>
        <a asp-action="Index">Back to Home</a>
    </div>

